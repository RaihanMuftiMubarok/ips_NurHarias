<div class="container mt-4">
    <h4 class="text-success fw-bold">Jadwal Latihan</h4>
    <div class="row">
        <% if (typeof data !== 'undefined' && data.length > 0) { %>
            <% let groupedData = {}; %>
            <% data.forEach(item => { %>
                <% if (!groupedData[item.minggu]) { groupedData[item.minggu] = []; } %>
                <% groupedData[item.minggu].push(item); %>
            <% }); %>

            <% Object.keys(groupedData).forEach(minggu => { %>
                <div class="col-md-6 col-lg-4 mb-3">
                    <div class="card shadow-sm">
                        <div class="card-header text-white text-center fw-bold" >
                            Minggu <%= minggu %>
                        </div>
                        <div class="card-body">
                            <% groupedData[minggu].forEach(lat => { %>
                                <p class="fw-bold text-success"><%= lat.hari %></p>
                                <p><%= lat.nama_latihan %></p>
                                <p><%= lat.jam_mulai %> - <%= lat.jam_selesai %></p>
                                <hr>
                            <% }) %>
                        </div>
                    </div>
                </div>
            <% }) %>
        <% } else { %>
            <p class="text-center">Tidak ada jadwal latihan.</p>
        <% } %>
    </div>
</div>


